<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".MembershipSettingActivity">

    <ImageButton
        android:id="@+id/backBtn"
        android:layout_width="7dp"
        android:layout_height="14dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="68dp"
        android:background="@color/white"
        android:padding="0dp"
        android:scaleType="centerCrop"
        android:src="@drawable/backbtn"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </ImageButton>

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="107dp"
        android:layout_marginEnd="44dp"
        android:text="최근 로그인한 계정이 기억나지\n않는다면 이렇게 해보세요!"
        android:textSize="20dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.063"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </TextView>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginTop="12dp"
        android:text="해당 내용을 참고하여 연동 현황을 확인해보세요!"
        android:textSize="14dp"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6">

    </TextView>

    <TextView
        android:id="@+id/kakao"
        android:layout_width="52dp"
        android:layout_height="21dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="12dp"
        android:text="카카오톡"
        android:textSize="14dp"
        android:textColor="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7">

    </TextView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="97dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="17dp"
        android:background="@drawable/solid_blue"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/kakao">


        <TextView
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="87dp"
            android:layout_marginStart="28dp"
            android:layout_marginTop="8dp"
            android:background="@color/blue"
            android:paddingTop="10dp"
            android:text="카카오톡 > 더보기 > 설정\n> 카카오계정\n> 연결된 서비스 관리"
            android:textSize="16dp"
            android:textColor="@color/white"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/kakao">

        </TextView>

    </LinearLayout>

    <TextView
        android:id="@+id/naver"
        android:layout_width="52dp"
        android:layout_height="21dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="121dp"
        android:text="네이버"
        android:textColor="@color/black"
        android:textSize="14dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/kakao">

    </TextView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="97dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="17dp"
        android:background="@drawable/solid_blue"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/naver">


        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="87dp"
            android:layout_marginStart="28dp"
            android:layout_marginTop="8dp"
            android:background="@color/blue"
            android:paddingTop="10dp"
            android:text="네이버 > 더보기 > 설정\n> 네이버계정\n> 연결된 서비스 관리"
            android:textColor="@color/white"
            android:textSize="16dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/naver">

        </TextView>

    </LinearLayout>


    <TextView
        android:id="@+id/google"
        android:layout_width="52dp"
        android:layout_height="21dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="121dp"
        android:text="네이버"
        android:textColor="@color/black"
        android:textSize="14dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/naver">

    </TextView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="97dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="17dp"
        android:background="@drawable/solid_blue"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/google">


        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="87dp"
            android:layout_marginStart="28dp"
            android:layout_marginTop="8dp"
            android:background="@color/blue"
            android:paddingTop="10dp"
            android:text="구글 > 더보기 > 설정\n> 구글계정\n> 연결된 서비스 관리"
            android:textColor="@color/white"
            android:textSize="16dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/google">

        </TextView>

    </LinearLayout>



</androidx.constraintlayout.widget.ConstraintLayout>